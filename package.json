{
  "name": "dungeon-master",
  "version": "1.0.0",
  "proxy": "http://localhost:3001/",
  "description": "D&amp;D Character tracker",
  "main": "server.js",
  "dependencies": {
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "express-jwt": "^5.3.1",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.4",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {},
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crashley1992/Dungeon-Master.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/crashley1992/Dungeon-Master/issues"
  },
  "homepage": "https://github.com/crashley1992/Dungeon-Master#readme"
}
